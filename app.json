{
  "expo": {
    "name": "ECG Monitor",
    "slug": "ecg-firebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo2.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo2.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yakap.ecgmonitor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo2.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yakap.ecgmonitor",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo2.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-updates"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bea1e311-496a-4913-bb4d-8d6db7dd83a6"
      }
    },
    "owner": "mjb123test",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/bea1e311-496a-4913-bb4d-8d6db7dd83a6"
    }
  }
}
